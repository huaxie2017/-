/* common style */
.f28 {
  font-size: 28rpx;
}
.f24 {
  font-size: 24rpx;
}
.f30 {
  font-size: 30rpx;
}
.col-0b {
  color: #0B0B0B;
}
.col-98 {
  color: #989898;
}
.col-blue {
  color: #3EA6EF;
}
.col-warning {
  color: #FF411B;
}
.fl-r {
  margin-left: auto;
}
.marl-8 {
  margin-left: 8rpx;
}
.bold {
  font-weight: bold;
}
.align-midd {
  vertical-align: middle;
}
.wid100 {
  width: 100%;
}
.btn-submit {
  border: none;
  width: 700rpx;
  height: 90rpx;
  margin-bottom: 82rpx;
  background:rgba(255,228,1,1);
  box-shadow:0px 18rpx 22rpx 0px rgba(255,210,1,0.3);
  border-radius:45rpx;
  letter-spacing: 4rpx;
}
.btn-submit::after {
  border: none;
}
.pay-order-container
 {
   padding: 0 26rpx;
 }
/* news start */
.news {
  width: 696rpx;
  height: 64rpx;
  line-height:64rpx;
  margin: 20rpx auto;
  color: #989898;
  font-size: 24rpx;
  background:rgba(238,238,238,1);
  border-radius:32rpx;
  text-align: center;
}
/* news end */

/* order-detail start */
.order-detail {
  position: relative;
  box-sizing: border-box;
  width: 697rpx;
  margin:16rpx auto 34rpx;
  padding: 32rpx 40rpx;
  background:linear-gradient(-72deg,rgba(247,217,23,1),rgba(254,197,0,1));
  box-shadow:0px 4rpx 38rpx 0 rgba(60,60,58,0.1);
  border-radius:20rpx;
}
.message{
  display: flex;
  align-items: center;
}
.order-tips {
  color: #0B0B0B;
  font-size: 24rpx;
}
.icon-city {
  width: 23rpx;
  height: 37rpx;
}
.price {
  margin: 44rpx 0 22rpx;
  font-size: 60rpx;
  font-weight: bold;
}
.renew {
  padding: 0;
  position: absolute;
  bottom: 28rpx;
  right: 40rpx;
  display: inline-block;
  width: 164rpx;
  height: 60rpx;
  border:1px solid rgba(11,11,11,1);
  border-radius:30rpx;
  line-height: 58rpx;
  background: transparent;
}
.renew::after {
  border: none;
}
/* order-detail end */

/* recover start */
.headline {
  position: relative;
  padding-left: 30rpx;
  letter-spacing: 2rpx;
}
.headline::after {
  position: absolute;
  top: 50%;
  left: 0;
  content: '';
  width: 12rpx;
  height: 12rpx;
  margin-top: -6rpx;
  border-radius: 50%;
  background: #FFE401;
}
.recover-main {
  margin-top: 35rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 4px 38px 0px rgba(60,60,58,0.1);
  border-radius:20rpx;
  overflow: hidden;
}
.recover-tab {
  box-sizing: border-box;
  height: 88rpx;
  line-height: 88rpx;
  border-bottom: 1rpx solid #eee;
}
.icon-agree {
  width: 32rpx;
  height: 32rpx;
  margin-right: 12rpx;
}
.tab {
  height: 88rpx;
  box-sizing: border-box;
  /* padding: 0 40rpx; */
  color: #989898;
  text-align: center;
}
.tab-0 {
  width: 271rpx;
}
.tab-1 {
  width: 240rpx;
}
.tab-actived {
  color: #0b0b0b;
  background: #FFE401;
}
.tab_disable{
  color: #fff;
  background: #dcdcdc;
}
.store-address{
  padding:  34rpx 0;
  margin: 0 36rpx;
  border-bottom: 1rpx solid #eee;
}
.store-address:last-child {
  border-bottom: none;
}
.icon-tel {
  width: 26rpx;
  height: 42rpx;
}
.icon-warning {
  width: 33rpx;
  height: 33rpx;
  margin-right: 21rpx;
  vertical-align: middle;
}
.icon-arrow {
  width: 23rpx;
  height: 30rpx;
}
.icon-remind {
  width: 33rpx;
  height: 33rpx;
  vertical-align: middle;
  margin-right: 10rpx;
}
/* recover end */

/* agree-rule start */
.agreement {
  display: flex;
  padding:62rpx 0 98rpx;
}
/* agree-rule end */
.btn-select {
  display: inline-block;
  width: 33rpx;
  height: 33rpx;
  margin-right: 16rpx;
}
.btn-select-d {
  background: url(data:image/png;base64,);
  background-size: contain;
}
.btn-select-s {
  background: url(data:image/jpeg;base64,);
  background-size: contain;
}
.logistics-list {
  position: absolute;
  top: 0;
  right: 0;
  width: 100%;
  height: 110rpx;
  line-height: 110rpx;
  font-size: 30rpx;
  z-index: 2;
}
.origin {
  display: block;
  width: 100%;
  height: 110rpx;
  padding-left: 300rpx;
}
/*预约时时间模块*/
.fixed-mask{
	z-index: 3;
	position: fixed;
    width: 100%;
    height: 100%;
    left: 0;
    top: 0;
    background: #000;
    opacity: .3;
}
.bookingTime{
	position: fixed;
	left: 0;
	bottom: 0;
	z-index: 5;
	width: 100%;
}
.bookingTime .tit{
	background:#fafafa;
	font-size: 15px;
	line-height: 34px;
	height: 34px;
	padding-left: 20px;
}
.swiper-container {
  color: #fff;
  text-align: center;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.swiper-slide{
  width: 100%;
  /*height: 300%;*/
  overflow: hidden;
}
.swiper-container2 {
    width: 100%;    
    margin-left: auto;
    margin-right: auto;
    color: #343434;
}
.gallery-thumbs1{
	background: #fdfdfd;
}
.swiper-slide {
    background-size: cover;
    background-position: center;
}
.gallery-thumbs1 {
    /*height: 7.5%;*/
    box-sizing: border-box;
}
.gallery-thumbs1 .intro{
	padding-top: 7px;
	border-bottom: 3px solid #fff;
  height: 40px;	
}
.gallery-thumbs1 .intro.active{
	border-bottom: 3px solid #fdd100;
	color: #fdd100;
}
.gallery-thumbs1 .intro .mt{
	margin-bottom: 5px;
}
.gallery-thumbs .swiper-wrapper{   
   height: 50px;
   border-top: 1px solid #e5e5e5;
    border-bottom: 1px solid #e5e5e5;
}
.gallery-thumbs .swiper-slide {
  float: left;
    width: 14%;
    height: 50px;
     
    font-size: 12px;  
}
.gallery-top1 {
    background: #f2f2f2;
    width: 100%;
}
.gallery-top1 .swiper-slide .pd{
	padding: 20px;
}
.gallery-top1 .swiper-slide .t-item{
	width: 30%;
	height: 42px;
	/*line-height: 42px;*/
	font-size: 14px;
	color: #343434;
	background: #ffffff;
	border-radius: 3px;
	float: left;
	margin: 1.3%;
	position: relative;
	border: 1px solid #ffffff;
}
.gallery-top1 .swiper-slide .t-item .icon-dg{
	position: absolute;
	right: 0;
	bottom: 0;
	width: 18px;
	height: 18px;
	display: none;
}
.gallery-top1 .swiper-slide .t-item .p1{
	height: 60%;
	line-height: 34px;
}
.gallery-top1 .swiper-slide .t-item .p2{
	height: 40%;
	font-size: 12px;
}
.gallery-top1 .swiper-slide .t-item.noClick{
  background: #dcdcdc;
  color: #ffffff;
}
.gallery-top1 .swiper-slide .t-item .hid{
	display: none;
}
.gallery-top1 .swiper-slide .t-item.choiced{
	border: 1px solid #fdd000;
}
.gallery-top1 .swiper-slide .t-item.choiced .icon-dg{
	display: block;
}
.gallery-top1 .swiper-slide .full-date .p2{
	display: block;	
}
.gallery-top1 .swiper-slide .full-date{
	background: #e2e2e2;
	color: #9b9b9b;
	border: 1px solid #e2e2e2;
}
