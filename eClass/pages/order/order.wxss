.orderContent{
  width: 100%;
  padding-top: 44rpx;
}
.stepOne{
  display: block;
  width: 700rpx;
  height: 78rpx;
  margin: 0 auto;
}
.stepTwo{
  display: block;
  width: 701rpx;
  height: 73rpx;
  margin: 0 auto;
}
.stepThree{
  display: block;
  width: 701rpx;
  height: 70rpx;
  margin: 0 auto;
}
.orderInfo{
  width: 100%;
  padding: 0 26rpx;
  margin-top: 68rpx;
  box-sizing: border-box;
}
.address{
  width:100%;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 38rpx 0px rgba(60,60,58,0.1);
  border-radius:20rpx;
  padding-top:36rpx; 
  padding-bottom: 40rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
}
.add{
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 28rpx;
  color: #0b0b0b;
  margin-top: 28rpx;
}
.tel{
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
  font-size: 28rpx;
  color: #0b0b0b;
  margin-top: 26rpx;
}
.shopInfo{
  width:100%;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 38rpx 0px rgba(60,60,58,0.1);
  border-radius:20rpx;
  margin-top: 38rpx;
  padding-top: 36rpx;
  padding-left: 15rpx;
  box-sizing: border-box;
}
.shopMessage{
  margin-top: 30rpx;
  padding-bottom: 18rpx;
  border-bottom: 1rpx solid #eee;
}
.phone{
  display: block;
  width: 106rpx;
  height: 98rpx;
  margin-right: 18rpx;
  padding-left: 30rpx;
}
.phoneName{
  font-size: 28rpx;
  color: #0b0b0b;
  font-weight: 500;
}
.source{
  font-size: 24rpx;
  color: #989898;
  margin-top: 24rpx;
}
.price{
  height: 80rpx;
  line-height: 80rpx;
  width: 100%;
  box-sizing: border-box;
}
.name{
  color: #0b0b0b;
  font-size: 30rpx;
  font-weight: 500;
  padding-left: 30rpx;
  text-align: left;
}
.money{
  font-size: 30rpx;
  color: #ff5534;
  font-weight: 500;
  padding-right: 26rpx;
  text-align: right;
}
.addressInfo{
  width:100%;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 38rpx 0px rgba(60,60,58,0.1);
  border-radius:20rpx;
  box-sizing: border-box;
  padding: 42rpx 0 45rpx 48rpx;
  margin-bottom: 80rpx;
  margin-top: 38rpx;
}
.infoName{
  font-size: 28rpx;
  color: #989898;
}
.infoM{
  font-size: 28rpx;
  color: #0b0b0b;
}
.footer{
  width: 100%;
  text-align: right;
  padding-top: 10rpx;
  padding-bottom: 80rpx;
  background:rgba(255,255,255,1);
  box-shadow:0px 4px 38px 0px rgba(60,60,58,0.1);
}
.cancel{
  width:231rpx;
  height:81rpx;
  border:1rpx solid rgba(228,228,228,1);
  border-radius:41rpx;
  display: inline-block;
  font-size: 30rpx;
  color: #989898;
  font-weight: 500;
  text-align: center;
  line-height: 81rpx;
  margin-right: 26rpx;
}
.orderSuccess{
  width: 100%;
  height: 263rpx;
  display: block;
}
.successText{
  font-size: 36rpx;
  color: #0b0b0b;
  font-weight: 800;
  margin-top: 20rpx;
  display: block;
  margin-bottom: 30rpx;
  text-align: center;
}
.successInfo{
  text-align: center;
}
.successPrice{
  font-size: 30rpx;
  color: #0b0b0b;
  font-weight: 500;
}
.successMoney{
  font-size: 36rpx;
  color: #ff5534;
  font-weight: 800;
}
.commentBtn{
  width:100%;
  height:89rpx;
  background:rgba(255,228,1,1);
  box-shadow:0px 18rpx 22rpx 0px rgba(255,210,1,0.3);
  border-radius:45rpx;
  font-size: 34rpx;
  font-weight: 800;
  color: #0b0b0b;
  text-align: center;
  line-height: 89rpx;
  margin-top: 56rpx;
}
.waitContent{
  width:100%;
  background:rgba(255,255,255,1);
  box-shadow:0px 4rpx 38rpx 0px rgba(60,60,58,0.1);
  border-radius:20rpx;
  margin-bottom: 38rpx;
  padding-top: 36rpx;
  padding-left: 15rpx;
  padding-bottom: 38rpx;
  box-sizing: border-box;
}
.waitText{
  width: 100%;
  box-sizing: border-box;
  padding-left: 30rpx;
  padding-right: 15rpx;
  font-size: 28rpx;
  line-height: 48rpx;
  color: #989898;
  margin-top: 24rpx;
}
.giveShow{
  text-align: center;
  margin-bottom: 40rpx;
}
.giveUp{
  display: inline-block;
  width: 70rpx;
  height: 70rpx;
  vertical-align: middle;
}
.giveText{
  font-size: 36rpx;
  color: #000;
  font-weight: 800;
  margin-left: 26rpx;
  vertical-align: middle;
}
.steps{
  width: 100%;
  box-sizing: border-box;
  padding: 0 26rpx;
}
.steps_mail{
  width: 100%;
}
.step_d{
  width: 100%;
  font-size: 24rpx;
  color: #989898;
}
.step_line{
  width: 100%;
  height: 14rpx;
  background: #e5e6e8;
  position: relative;
  margin-bottom: 12rpx;
}
.circle{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  left: 0;
  margin-top: -15rpx;
  z-index: 1;
  background: #e5e6e8;
  border-radius: 50%;
}
.circle_finish{
  width: 30rpx;
  height: 30rpx;
  position: absolute;
  top: 50%;
  right: 0;
  margin-top: -15rpx;
  z-index: 1;
  background: #e5e6e8;
  border-radius: 50%;
}
.m-left{
  margin-left: -15rpx;
}
.m-right{
  text-align: right;
}
.wait_order{
  text-align: left;
  margin-left: -15rpx;
}
.wait_order.finish_step{
  color: #000;
}
.finish_order{
  text-align: right;
  margin-right: -15rpx;
}
.finish_order.finish_step{
  color: #000;
}
.step_d.finish_step{
  color: #000;
}
.step_line.finish_step{
  background: #ffe401;
}
.circle.finish_step{
  background: #ffe401 url(data:image/png;base64,)no-repeat center center;
  background-size: 20rpx 14rpx;
}
.steps_home{
  width: 400rpx;
  margin: 0 auto;
}
.circle_finish.finish_step{
  background: #ffe401 url(data:image/png;base64,)no-repeat center center;
  background-size: 20rpx 14rpx;
}
.lookDetail{
  color: #333;
  margin-left: 20rpx;
}
.fix_bg{
  width: 100%;
  height: 100%;
  background: rgba(0,0,0,0.6);
  position: fixed;
  top: 0;
  left: 0;
  bottom: 0;
  right: 0;
  z-index: 10;
}
.phoneContent{
  width: 100%;
  background: #fff;
  position: absolute;
  bottom: 0;
  left: 0;
  right: 0;
  box-sizing: border-box;
}
.title_top{
  height: 80rpx;
  line-height: 80rpx;
  position: relative;
  text-align: center;
  font-size: 28rpx;
  color: #333;
  background: #dcdcdc;
}
.closeDetail{
  display: block;
  color: #333;
  font-size: 24rpx;
  position: absolute;
  top: 50%;
  left: 24rpx;
  height: 80rpx;
  margin-top: -40rpx;
}
.detail_li{
  width: 100%;
  height: 80rpx;
  line-height: 80rpx;
  box-sizing: border-box;
  padding: 0 24rpx;
}
.buyWay{
  font-size: 24rpx;
  color: #999;
}
.detail_li .info{
  font-size: 24rpx;
  color: #333;
  text-align: right;
}
