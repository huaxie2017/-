.align-mid
 {
   vertical-align: middle;
 }
.fl-r {
  margin-left: auto;
}
.marl-10 {
  margin-left: 10rpx;
}
.f34 {
  font-size: 34rpx;
}
.bold {
  font-weight: bold;
}
.col-0b {
  color: #0b0b0b;
}
.btn-submit {
  border: none;
  width: 700rpx;
  height: 90rpx;
  background:rgba(255,228,1,1);
  box-shadow:0px 18rpx 22rpx 0px rgba(255,210,1,0.3);
  border-radius:45rpx;
  letter-spacing: 4rpx;
}
.btn-submit::after {
  border: none;
}
.progress {
  box-sizing: border-box;
  position: fixed;
  top: 0;
  width: 100%;
  height: 64rpx;
  padding: 0 40rpx;
  background: #fff;
  box-shadow: 0 0 40rpx rgba(60,60,58,0.3);
  z-index: 2;
  overflow: hidden;
}
.valuation-body {
  padding: 64rpx 0 257rpx;
}
.question-item {
  padding: 0 30rpx;
  margin: 26rpx auto;
}
.title {
  margin-bottom: 30rpx;
  color: #666;
  font-size: 30rpx;
  letter-spacing: 2rpx;
}
.content {
  background:rgba(255,255,255,1);
  box-shadow:0px 4px 38px 0px rgba(60,60,58,0.1);
  border-radius: 20rpx;
}
.content-item {
  display: flex;
  align-items: center;
  padding: 32rpx 50rpx;
  color: #0B0B0B;
  font-size: 30rpx;
  border-bottom: 1rpx solid #EDEDEC;
}
.content-item:last-child {
  border-bottom: none; 
}
.tips {
  margin: 0; 
  padding-top: 6rpx;
  color: #989898;
  font-size: 24rpx;
}
.icon-q{
  width: 33rpx;
  height: 33rpx;
}
.valuation-submit {
  box-sizing: border-box;
  position: fixed;
  bottom: 0;
  left: 0;
  width: 100%;
  height: 189rpx;
  padding-top: 20rpx;
  background: #fff;
  box-shadow:0px 4px 38px 0px rgba(60,60,58,0.1);
}
.btn-select {
  width: 42rpx;
  height: 42rpx;
  margin-right: 34rpx;
}
.btn-select-d {
  background: url(data:image/png;base64,);
  background-size: contain;
}
.btn-select-s {
  background: url(data:image/jpeg;base64,);
  background-size: contain;
}
.progress-txt{
  position: absolute;
  line-height: 64rpx;
  letter-spacing: 4rpx;
  z-index: 3;
}
.progress-rate {
  position: absolute;
  top: 0;
  left: 0;
  /* width: 322rpx; */
  height: 64rpx;
  background: #FFE401;
  border-top-right-radius: 32rpx;
  border-bottom-right-radius: 32rpx;
}
.num {
  position: absolute;
  top: 0;
  right: 40rpx;
  z-index: 3;
}